const express = require('express');
const app = express();

// Azure erwartet, dass du auf process.env.PORT hÃ¶rst
const port = process.env.PORT || 8080;

app.get('/', (_, res) => {
  res.send('Hello from Immoleaf backend!');
});

app.listen(port, () => {
  console.log(`Server is running on port ${port}`);
});
